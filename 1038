#include <stdlib.h>
#include <stdio.h>


int main () {
	int codigo, quantidade;
	double total;
	
scanf("%i %i", &codigo, &quantidade);	
	if (codigo==1)
	{total=4*quantidade;}
		else if (codigo==2)
		{total=4.5*quantidade;}
			else if (codigo==3)
			{total=5*quantidade;}
				else if (codigo==4)
				{total=2*quantidade;}
					else if (codigo==5)
					{total=1.5*quantidade;}

printf("Total: R$ %.2lf\n", total);

return 0;

}v
